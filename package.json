{
  "name": "brindillette",
  "version": "0.1.0",
  "description": "serverless version of Brindille",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "precommit": "lint-staged",
    "lint": "prettier --single-quote --no-semi --write 'src/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "author": "Hellohikimori",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.3",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "brindille-component": "*",
    "brindille-interactive-component": "*",
    "brindille-resize": "*",
    "brindille-scroll": "*",
    "brindille-stylus-extends": "*",
    "component-emitter": "^1.2.1",
    "dom-classes": "*",
    "dom-event": "*",
    "emitter-component": "*",
    "stylus-easing": "^1.0.0",
    "stylus-font-face": "*",
    "stylus-media-queries": "^1.0.0",
    "stylus-reset": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brindille/brindille.git"
  },
  "homepage": "https://github.com/brindille/brindille",
  "bugs": {
    "url": "https://github.com/brindille/brindille/issues"
  }
}
